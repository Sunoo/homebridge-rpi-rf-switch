{
  "displayName": "Homebridge rpi-rf Switch",
  "name": "homebridge-rpi-rf-switch",
  "version": "2.2.0",
  "description": "rpi-rf plugin for Homebridge",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Sunoo/homebridge-rpi-rf-switch.git"
  },
  "keywords": [
    "homebridge-plugin",
    "rpi-rf",
    "433",
    "433Mhz"
  ],
  "author": "David Maher",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Sunoo/homebridge-rpi-rf-switch/issues"
  },
  "homepage": "https://github.com/Sunoo/homebridge-rpi-rf-switch#readme",
  "scripts": {
    "clean": "rimraf ./dist ./coverage",
    "build": "rimraf ./dist ./coverage && tsc",
    "format": "markdownlint --fix *.md",
    "lint": "eslint src/*.ts --fix",
    "lint-check": "eslint src/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint-check",
    "postpublish": "npm run clean",
    "watch": "npm run clean && tsc --watch"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "@types/node": "14.0.25",
    "eslint": "^7.5.0",
    "homebridge": "^1.1.1",
    "markdownlint-cli": "^0.23.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  },
  "files": [
    "config.schema.json",
    "dist/**/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=10",
    "homebridge": ">=1.0.0"
  },
  "dependencies": {
    "node-calls-python": ">=1.4.0"
  }
}
